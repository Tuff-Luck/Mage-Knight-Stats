									if hexLocation=="center" then params.position={obj.getPosition()[1]-0.1, y, obj.getPosition()[3]-0.1}
									else params.position={angleToXY(obj,hexLocation)[1]-0.1, y ,angleToXY(obj,hexLocation)[2]-0.1} end
