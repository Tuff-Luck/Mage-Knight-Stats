									{-18.00, 1.25, -27.50}, {-60.00, 1.00, -38.00}, { 12.10, 1.25,  24.70}, { 40.15, 1.25,  18.80}, {-66.20, 1.16, -33.82}, {-41.76, 1.16, -32.78},
									{-63.49, 1.50, -31.19},	{-68.04, 1.10, -34.03}}
